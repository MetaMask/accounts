{
  "name": "@metamask/webview-crypto",
  "version": "1.0.0",
  "description": "A simple standard interface for a seed phrase generated set of Ethereum accounts.",
  "keywords": [
    "ethereum",
    "keyring"
  ],
  "homepage": "https://github.com/MetaMask/accounts/tree/main/packages/webview-crypto#readme",
  "bugs": {
    "url": "https://github.com/MetaMask/accounts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/accounts.git"
  },
  "license": "ISC",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./srcWebview/webviewCrypto.html": {
      "types": "./srcWebview/webviewCrypto.html",
      "default": "./srcWebview/webviewCrypto.html"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist/",
    "srcWebview/"
  ],
  "scripts": {
    "build": "ts-bridge --project tsconfig.build.json --no-references && yarn build:webview",
    "build:clean": "yarn build --clean",
    "build:webview": "node ./build-webview.js",
    "build:docs": "typedoc",
    "changelog:update": "../../scripts/update-changelog.sh @metamask/eth-hd-keyring",
    "changelog:validate": "../../scripts/validate-changelog.sh @metamask/eth-hd-keyring",
    "publish:preview": "yarn npm publish --tag preview",
    "test": "jest",
    "test:clean": "jest --clearCache"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@metamask/react-native-webview": "^14.2.2",
    "ethereum-cryptography": "^2.1.2"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^3.2.1",
    "@lavamoat/preinstall-always-fail": "^2.1.0",
    "@metamask/auto-changelog": "^3.4.4",
    "@metamask/bip39": "^4.0.0",
    "@ts-bridge/cli": "^0.6.3",
    "@types/babel__plugin-transform-runtime": "^7",
    "@types/babel__preset-env": "^7",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "babel-loader": "^10.0.0",
    "deepmerge": "^4.2.2",
    "jest": "^29.5.0",
    "react-native": "0.80.1",
    "typescript": "~5.6.3",
    "webpack": "^5.100.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": "^18.18 || >=20"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "lavamoat": {
    "allowScripts": {
      "@lavamoat/preinstall-always-fail": false,
      "@metamask/webview-crypto>ethereumjs-util>ethereum-cryptography>keccak": false,
      "@metamask/webview-crypto>ethereumjs-util>ethereum-cryptography>secp256k1": false
    }
  }
}
